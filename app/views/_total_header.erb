<tr>
  <th colspan="2" class="span3"><br/></th>
  <% case
  when gvtype_fe? %>
    <th class="well span1">Total</th>
    <% ROGv::FortUtil.fort_types_fe.each do |f| %>
      <th class="span1"><%= f %></th>
    <% end %>
    <th class="well span1">FE</th>
    <% ROGv::FortUtil.fort_types_se.each do |f| %>
      <th class="span1"><%= f %></th>
    <% end %>
    <th class="well span1">SE</th>
  <% when gvtype_te? %>
    <th class="well span1">Total</th>
    <% ROGv::FortUtil.fort_types_te.each do |f| %>
      <th class="span1"><%= f %></th>
    <% end %>
  <% end %>
</tr>
